		return "Swagger Hello World";
	}
	
	@RequestMapping(method = RequestMethod.POST, value = "/api/getNewRegisterCount")//getUserProfile
	public String getNewRegisterCount() {
		return "Swagger Hello World";
	}
	
	@RequestMapping(method = RequestMethod.POST, value = "/api/sendMessage")//getUserProfile
	public String sendMessage() {
		return "Swagger Hello World";
	}
	
	@RequestMapping(method = RequestMethod.POST, value = "/api/sendNotification")//getUserProfile
	public String sendNotification() {
		return "Swagger Hello World";
	}
	
	@RequestMapping(method = RequestMethod.POST, value = "/api/getPatients")//getUserProfile
