		return "Swagger Hello World";
	}
	
	@RequestMapping(method = RequestMethod.POST, value = "/api/getHighAlertCount")//getUserProfile
	public String getHighAlertCount() {
		return "Swagger Hello World";
	}
	@RequestMapping(method = RequestMethod.POST, value = "/api/getPatientDetails")//getUserProfile
